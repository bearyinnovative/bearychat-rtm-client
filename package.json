{
  "name": "bearychat-rtm-client",
  "version": "0.4.0",
  "description": "RTM client for BearyChat",
  "main": "lib/index.js",
  "repository": "https://github.com/bearyinnovative/bearychat-rtm-client",
  "author": "Yue Wang <wangyue2002@gmail.com>",
  "license": "MIT",
  "optionalDependencies": {},
  "scripts": {
    "test": "jest",
    "clean": "rm -f *.js",
    "build:babel": "babel src -d . --ignore src/__test__/*.js --source-maps inline",
    "build": "npm run clean && npm run build:babel",
    "build:doc": "markdown-toc -i README.md",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "delay": "^2.0.0",
    "invariant": "^2.2.2"
  },
  "keywords": [
    "BearyChat",
    "API",
    "chatops"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "jest": "^20.0.3",
    "markdown-toc": "^1.1.0",
    "mock-socket": "^6.0.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "__test__"
    ],
    "setupFiles": [
      "./src/__test__/prepare.js"
    ]
  }
}
